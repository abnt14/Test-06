<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<object id="janela" class="GtkWindow">
		<property name="title">Programa 06</property>
		<property name="default_width">400</property>
		<property name="default_height">400</property>
		<child>
			<object id="grid" class="GtkGrid">
				<property name="halign">center</property>
				<property name="valign">center</property>
				<property name="column-spacing">8</property>
				<child>
					<object id="scrol" class="GtkScrolledWindow">
						<child>
							<object id="isrTexto" class="GtkTextView">
								<layout>
									<property name="column">0</property>
                                                        		<property name="row">0</property>
								</layout>
							</object>
						</child>
					</object>
				</child>
				<child>
					<object id="botao01" class="GtkButton">
						<property name="label">Exit</property>
						<layout>
							<property name="column">0</property>
							<property name="row">0</property>
						</layout>
					</object>
				</child>
			</object>
		</child>
	</object>
</inferface>
